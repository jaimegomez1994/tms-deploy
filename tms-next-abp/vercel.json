{
    "version": 2,
    "builds": [
        {
            "src": "apps/abp/package.json",
            "use": "@vercel/next",
            "config": {
                "installCommand": "pnpm install",
                "buildCommand": "pnpm run build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "apps/abp/$1"
        }
    ]
}
