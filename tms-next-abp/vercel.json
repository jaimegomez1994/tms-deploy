{
    "version": 2,
    "builds": [
        {
            "src": "apps/abp/next.config.js",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/apps/abp/$1"
        }
    ],
    "buildCommand": "turbo run build",
    "installCommand": "pnpm install",
    "outputDirectory": "apps/abp/.next"
}
